<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InputMenu">
   <children>
      <Label fx:id="TheOpeningLabel" layoutX="123.0" layoutY="40.0" prefHeight="41.0" prefWidth="354.0" text="Congratulations on completing a match.  What was the result?" />
      <Label layoutX="51.0" layoutY="165.0" text="I played as white and:" />
      <Label layoutX="52.0" layoutY="249.0" text="I played as black and:" />
      <Button fx:id="TheSubmitButton" layoutX="274.0" layoutY="318.0" mnemonicParsing="false" onAction="#onTheSubmitButtonAction" text="Submit" />
      <TextField fx:id="TheMyEloField" layoutX="174.0" layoutY="113.0" prefHeight="25.0" prefWidth="100.0" promptText="My Elo" />
      <TextField fx:id="TheOppEloField" layoutX="332.0" layoutY="113.0" prefHeight="25.0" prefWidth="100.0" promptText="Opponent's Elo" />
      <RadioButton fx:id="TheWhiteWonRadioButton" layoutX="174.0" layoutY="165.0" mnemonicParsing="false" onAction="#onTheWhiteWonRadioButtonAction" text="I won" />
      <RadioButton fx:id="TheWhiteDrawRadioButton" layoutX="274.0" layoutY="165.0" mnemonicParsing="false" onAction="#onTheWhiteDrawRadioButtonAction" text="Draw" />
      <RadioButton fx:id="TheWhiteLostRadioButton" layoutX="376.0" layoutY="165.0" mnemonicParsing="false" onAction="#onTheWhiteLostRadioButtonAction" text="I lost" />
      <RadioButton fx:id="TheBlackWonRadioButton" layoutX="174.0" layoutY="249.0" mnemonicParsing="false" onAction="#onTheBlackWonRadioButtonAction" text="I won" />
      <RadioButton fx:id="TheBlackDrawRadioButton" layoutX="274.0" layoutY="249.0" mnemonicParsing="false" onAction="#onTheBlackDrawRadioButtonAction" text="Draw" />
      <RadioButton fx:id="TheBlackLostRadioButton" layoutX="376.0" layoutY="249.0" mnemonicParsing="false" onAction="#onTheBlackLostRadioButtonAction" text="I lost" />
      <ComboBox fx:id="TheOpeningBox" layoutX="52.0" layoutY="318.0" prefWidth="150.0" promptText="Opening played..." />
      <Button fx:id="TheBackButton" layoutX="513.0" layoutY="337.0" mnemonicParsing="false" onAction="#onTheBackButtonAction" text="Back" />
   </children>
</AnchorPane>
